@{
}
@model DisplayEmployee
@if (ViewBag.Massege != null)
{
    <div class="card">
        <div class="card-body">
            @ViewBag.Massege
        </div>
    </div>
}
else
{
<div class="card">
    <div class="card-header">
        User Iformation
    </div>
        <img src="~/@ViewBag.Employee.ImagePath" class="card-img-top" alt="...">
    <div class="card-body">
    <ul class="list-group list-group-flush">
        <li class="list-group-item"> First Name : @ViewBag.Employee.FName</li>
        <li class="list-group-item"> Last Name : @ViewBag.Employee.LName</li>
        <li class="list-group-item"> Age : @ViewBag.Employee.Age</li>
        <li class="list-group-item"> Created On: @ViewBag.Employee.CreatedOn</li>

    </ul>
    </div>
</div>
}
